{
  "name": "marked-input",
  "private": true,
  "version": "2.0.0",
  "description": "React component for combine editable text with any component using annotated text",
  "license": "MIT",
  "author": "Ruslan Ilyin",
  "scripts": {
    "storybook": "start-storybook -p 6006 -c storybook",
    "build-storybook": "build-storybook -c storybook",
    "build": "vite build && tsc && node prepack.js",
    "test": "npm run test -w tests",
    "test:integration": "npm run test:integration -w tests",
    "coverage": "jest -c tests/jest.config.json --coverage",
    "app": "vite tests/e2e/app -c vite.config.ts --open"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@types/react": "^18.0.18",
    "@vitejs/plugin-react": "^2.0.0",
    "babel-loader": "^8.2.5",
    "typescript": "^4.8.3",
    "vite": "^3.1.1",
    "vite-plugin-css-injected-by-js": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nowely/marked-input.git"
  },
  "type": "module",
  "types": "./types/index.d.ts",
  "module": "./index.js",
  "main": "./index.umd.cjs",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.umd.cjs"
    }
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "mention",
    "markup",
    "marked-input",
    "highlight",
    "input"
  ],
  "workspaces": [
    "app",
    "lib",
    "storybook",
    "tests"
  ]
}
